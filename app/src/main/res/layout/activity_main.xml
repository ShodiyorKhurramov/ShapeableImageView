<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:orientation="vertical">
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"
            app:shapeAppearanceOverlay="@style/ImageStyle_Circle"/>
    <com.google.android.material.imageview.ShapeableImageView
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_margin="5dp"
        android:padding="5dp"
        android:scaleType="centerCrop"
        android:src="@drawable/kelliy_image"
        app:shapeAppearanceOverlay="@style/ImageStyle_Circle"
        app:strokeWidth="6dp"
        app:strokeColor="@color/black"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"
            app:shapeAppearanceOverlay="@style/ImageStyle_Corners_Rounded"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"
            app:shapeAppearanceOverlay="@style/ImageStyle_Corners_Rounded"
            app:strokeWidth="6dp"
            app:strokeColor="@color/black"/>

        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"
            app:shapeAppearanceOverlay="@style/ImageStyle_Corners_Cut"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"
            app:shapeAppearanceOverlay="@style/ImageStyle_Corners_Cut"
            app:strokeWidth="6dp"
            app:strokeColor="@color/black"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"
            app:shapeAppearanceOverlay="@style/ImageStyle_Corners_Square"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"
            app:shapeAppearanceOverlay="@style/ImageStyle_Corners_Square"
            app:strokeWidth="6dp"
            app:strokeColor="@color/black"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"
            app:shapeAppearanceOverlay="@style/ImageStyle_Corners_Rounded_Top_Right"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"
            app:shapeAppearanceOverlay="@style/ImageStyle_Corners_Rounded_Top_Right"
            app:strokeWidth="6dp"
            app:strokeColor="@color/black"/>
        />

        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"
            app:shapeAppearanceOverlay="@style/ImageStyle_Cut_Rounded_Top_Right_Bottom_Left"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"

            app:shapeAppearanceOverlay="@style/ImageStyle_Cut_Rounded_Top_Right_Bottom_Left"
            app:strokeWidth="6dp"
            app:strokeColor="@color/black"/>
        <com.google.android.material.imageview.ShapeableImageView
            android:id="@+id/shapeable"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:padding="5dp"
            android:scaleType="centerCrop"
            android:src="@drawable/kelliy_image"/>



</LinearLayout>
</ScrollView>